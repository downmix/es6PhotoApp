{{#each albumList}}
<div class="mdl-cell mdl-cell--3-col">
  <div class="demo-card-square mdl-card mdl-shadow--2dp" style="height:320px;">
    <div class="mdl-card__title mdl-card--expand" style="background:
url('//lorempixel.com/330/217/?{{id}}') 0 0 no-repeat #46B6AC;">
      <h2 class="mdl-card__title-text">{{ id }}</h2>
    </div>
    <div class="mdl-card__supporting-text">
      {{ title }}
    </div>
    <div class="mdl-card__actions mdl-card--border">
      <a class="mdl-button mdl-button--colored mdl-js-button mdl-js-ripple-effect">
        View Updates
      </a>
    </div>
  </div>
</div>
{{/each}}
